;
; G - Code configuration file
;
; History
;
; 
; ====================================  ========== =================================================================================
; Kanstantsin Piatraska Fattum@3dpm.ru  19/12/2023 Постпроцессор для стойки Syntec 60W-E и 6MD под ArtCam 2018 со сменой инструмента
; 

;
DESCRIPTION = "Syntec 60w-e and 6mb (mm) (*.CNC)"
;
FILE_EXTENSION = "cnc"
;
UNITS = MM
;
; Cariage return - line feed at end of each line
;
END_OF_LINE = "[13][10]"
;
; Block numbering 
;
LINE_NUM_START     = 0
LINE_NUM_INCREMENT = 10
LINE_NUM_MAXIMUM   = 999999
;
; Set up default formating for variables
;
; Line numbering
FORMAT = [N|@|N|1.0]
; Spindle Speed
FORMAT = [S|@|S|1.0]
; Feed Rate
FORMAT = [F|#|F|1.1]
; Tool moves in x,y and z
FORMAT = [X|#|X|1.3]
FORMAT = [Y|#|Y|1.3]
FORMAT = [Z|#|Z|1.3]
; Arc Centre Cordinates
FORMAT = [I|@|I|1.3]
FORMAT = [J|@|J|1.3]
; Home tool positions 
FORMAT = [XH|@|X|1.3]
FORMAT = [YH|@|Y|1.3]
FORMAT = [ZH|@|Z|1.3]
;
; Set up program header
;
START = "([FILENAME])"
START = "(X=[XSIZE], Y=[YSIZE], Z=[ZMATERIAL])"
START = "G54"
START = "G90"
START = "M10"
START = "M459 M457 M479 M477"
START = "M98 P[T] ([TOOLDESC])"
START = "G00[ZH]"
;START = "G00[XH][YH]"
;
; Toolchange Sections
TOOLCHANGE ="M98 P[T] ([TOOLDESC])" 
;
; Program moves
;
RAPID_RATE_MOVE        = "G00[X][Y][Z]"
;
FIRST_FEED_RATE_MOVE   = "G01[X][Y][Z][F]"
FEED_RATE_MOVE         = "G01[X][Y][Z]"
;
;FIRST_CW_ARC_MOVE      = "G02[X][Y][I][J][F]"
;CW_ARC_MOVE            = "G02[X][Y][I][J]"
;
;FIRST_CCW_ARC_MOVE     = "G03[X][Y][I][J][F]"
;CCW_ARC_MOVE           = "G03[X][Y][I][J]"
;
; End of file
;
END = "G00[ZH]"
END = "M11"
END = "M05"
END = "M98 P0300"
END = "M0"
END = "M200"
END = "M02"
END = "%"